{
    "name": "rmzamora/formatter-bundle",
    "type": "symfony-bundle",
    "description": "Symfony SonataFormatterBundle rmzamora forked",
    "keywords": ["sonata", "formatter", "markdown", "ckeditor", "markitup", "rmzamora forked"],
    "homepage": "https://sonata-project.org/bundles/formatter",
    "license": "MIT",
    "authors": [
        {
            "name": "Mell M. Zamora",
            "email": "me@mellzamora.com",
            "homepage": "http://mellzamora.com"
        },
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "https://sonata-project.org"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataFormatterBundle/contributors"
        }
    ],
    "require": {
        "symfony/framework-bundle": "~2.3",
        "symfony/form": "~2.3",
        "symfony/property-access": "~2.3",
        "twig/twig": "*",
        "knplabs/knp-markdown-bundle": "~1.3",
        "egeloen/ckeditor-bundle": "~3.0",
        "rmzamora/block-bundle": "2.4.*",
        "sonata-project/core-bundle": "~2.2"
    },
    "require-dev": {
        "symfony/validator": "~2.3",
        "fabpot/php-cs-fixer": "~0.5|~1.0"
    },
    "autoload": {
        "psr-4": { "Sonata\\FormatterBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-2.4": "2.4.x-dev"
        }
    }
}
